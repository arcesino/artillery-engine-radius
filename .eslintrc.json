{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": 0,
        "func-names": 0,
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0,
        "no-shadow": 0
    }
}
